# Deny access to sensitive files
<FilesMatch "\.(sqlite|sqlite3|db|log|ini|json|sql)$">
    Require all denied
</FilesMatch>

# Disable directory listing
Options -Indexes

# Protect specific sensitive files explicitly
<Files "unipanel.sqlite">
    Require all denied
</Files>

<Files "template_hashes.json">
    Require all denied
</Files>
